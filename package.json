    "@prisma/client": "^6.3.1",
    "aspida": "^1.14.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "next": "^15.1.6",
    "node-fetch": "^2.6.1",
    "pathpida": "^0.24.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.3.0",
    "zod": "^3.24.1"
